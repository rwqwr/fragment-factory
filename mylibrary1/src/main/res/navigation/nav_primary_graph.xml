<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_primary_graph"
    app:startDestination="@id/primaryFragment">

    <include app:graph="@navigation/nav_secondary_host_graph" />

    <fragment
        android:id="@+id/primaryFragment"
        android:name="com.example.mylibrary1.PrimaryFragment"
        tools:layout="@layout/fragment_primary">
        <action
            android:id="@+id/action_primary_to_secondary"
            app:destination="@id/nav_secondary_host_graph" >

            <argument android:name="arg"
                app:argType="integer"
                android:defaultValue="1"/>
        </action>
    </fragment>

</navigation>